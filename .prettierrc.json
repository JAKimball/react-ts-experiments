{
  "$schema": "http://json.schemastore.org/prettierrc",
  "overrides": [{ "files": ".nvmrc", "options": { "parser": "yaml" } }],
  "plugins": ["prettier-plugin-packagejson"],
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "arrowParens": "avoid",
  "bracketSpacing": true
}
